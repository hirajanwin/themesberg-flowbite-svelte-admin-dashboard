{
	"name": "flowbite-svelte-admin-dashboard",
	"version": "0.0.3",
	"description": "Flowbite Admin Dashboard components for Svelte",
	"main": "index.js",
	"author": {
		"name": "Shinichi Okada",
		"email": "connect@codewithshin.com",
		"url": "https://blog.codewithshin.com"
	},
	"bugs": "https://github.com/themesberg/flowbite-svelte-admin-dashboard/issues",
	"homepage": "https://flowbite-svelte-admin-dashboard.vercel.app/",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:package": "svelte-kit sync && svelte-package",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"package:publish": "standard-version && git push --follow-tags origin main && npm run build:package && npm publish ./package"
	},
	"devDependencies": {
		"@playwright/test": "^1.40.1",
		"@popperjs/core": "^2.11.8",
		"@sveltejs/adapter-auto": "^3.0.1",
		"@sveltejs/kit": "^2.0.6",
		"@sveltejs/package": "^2.2.5",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@typescript-eslint/eslint-plugin": "^6.16.0",
		"@typescript-eslint/parser": "^6.16.0",
		"autoprefixer": "^10.4.16",
		"classnames": "^2.5.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"flowbite": "^2.2.1",
		"flowbite-svelte": "^0.44.21",
		"flowbite-svelte-icons": "^0.4.5",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"svelte-preprocess": "^5.1.3",
		"tailwindcss": "^3.4.0",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.10",
		"vitest": "^1.1.0"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/themesberg/flowbite-svelte-admin-dashboard"
	},
	"engines": {
		"npm": ">=7.0.0",
		"node": ">=16.0.0"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"tailwindcss",
		"flowbite",
		"ui",
		"signin",
		"signup"
	]
}
